/* pages/search/search.wxss */
page {
    width: 100%;
    height: 100%;
    background-color: #fff;
  
}
.container {
    margin: 0;
    padding: 0;
   
    width: 100%;
    height: 100%;
    overflow: hidden;
    box-sizing: border-box;
    
}

.weui__hd{
    height:100rpx;
    width: 100%;
    margin: 0;
    position: relative;
    overflow: hidden;
    display: flex;
    align-items: center;
    border-bottom: 1px solid #f3f3f3;

}
.weui-search-bar__box{
    width: 75%;
    height: 66rpx; 
   margin-left: 13rpx;
   background-color: #f5f5f5;
   border-radius: 5px;
   display: flex;
   align-items: center;
}
.weui-icon-search_in-box{
    margin-left: 10rpx;
    height: 25rpx;
    width: 25rpx;
    
}
.weui-search-bar__input{
    margin-left: 10rpx;
    font-size: 16px;
 

}
.weui-cancel__search {
    width: 10%;
    font-size: 16px;
    margin:auto 7.5% auto 7.5%;
    text-align: center;
    color: #7abbe2;
}